# === Student config ===
# ใส่เลข 4 ตัวท้ายรหัสนักศึกษา (ตัวอย่าง 0448)
SUFFIX=0079

# กรอกพอร์ตตามรูปแบบที่อาจารย์กำหนด (ต้อง >= 10000)
# ห้ามใช้อะไรนอกจากพอร์ตตนเอง
WEB_PORT=10079
DB_PORT=20079
REDIS_PORT=30079   # ถ้าโปรเจ็กต์ไม่มี Redis สามารถไม่ใช้ก็ได้ แต่ให้คงค่าพอร์ตไว้

# === Django ===
DJANGO_SETTINGS_MODULE=easyparking.settings
SECRET_KEY=replace-with-a-long-random-secret
DEBUG=0
ALLOWED_HOSTS=localhost,127.0.0.1,10.80.21.37

# === Database (PostgreSQL) ===
DB_ENGINE=django.db.backends.postgresql
DB_NAME=easyparking
DB_USER=postgres
DB_PASSWORD=changeme
DB_HOST=db
DB_PORT=5432   # พอร์ตภายในคอนเทนเนอร์ (คงค่า default 5432)
